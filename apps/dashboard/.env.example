# App Configuration
VITE_APP_NAME=SonicKid Dashboard
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_URL=http://localhost:3005

# ThirdWeb Configuration
VITE_THIRDWEB_CLIENT_ID=your_client_id_here
VITE_THIRDWEB_SECRET_KEY=your_secret_key_here

# Subgraph Configuration
VITE_SUBGRAPH_ENDPOINT=https://api.studio.thegraph.com/query/your-id/sonic-blockchain/version
VITE_SUBGRAPH_WS_ENDPOINT=wss://api.studio.thegraph.com/query/your-id/sonic-blockchain/version

# Telegram Configuration
VITE_TELEGRAM_BOT_USERNAME=your_bot_username
VITE_TELEGRAM_BOT_TOKEN=your_bot_token

# Chain Configuration
VITE_CHAIN_ID=1234
VITE_RPC_URL=your_rpc_url

# Feature Flags
VITE_ENABLE_TESTNET=true
VITE_ENABLE_SOCIAL_FEATURES=true
VITE_ENABLE_ANALYTICS=true

# Deployment
VITE_PUBLIC_URL=https://your-app-url.com
